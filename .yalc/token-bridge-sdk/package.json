    "ethers": "5.0.24",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isempty": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "typescript": "^3.8.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "files": [
    "dist/**/*"
  ],
  "peerDependencies": {
    "react": "^17.0.2",
