  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "description": "SDK for transfering assets to and from Arbitrum",
  "author": "Offchain Labs, Inc.",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OffchainLabs/token-bridge-sdk.git"
  },
  "bugs": {
